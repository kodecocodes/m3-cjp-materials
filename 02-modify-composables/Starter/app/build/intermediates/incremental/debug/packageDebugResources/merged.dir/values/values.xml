    <color name="colorPrimary">#FF5A00</color>
    <color name="colorPrimaryDark">#CE3F00</color>
    <color name="ic_launcher_background">#FFFFFF</color>
    <string name="app_name">Hello Compose</string>
    <style name="AppTheme" parent="android:Theme.Material.Light.NoActionBar">
    
